{
  "crons": [
    {
      "path": "/api/cron/publish-scheduled",
      "schedule": "0 0 * * *"
    }
  ],
  "functions": {
    "app/api/cron/publish-scheduled/route.ts": {
      "maxDuration": 60
    }
  },
  "headers": [
    {
      "source": "/api/cron/:path*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, no-cache, must-revalidate"
        }
      ]
    }
  ]
}
